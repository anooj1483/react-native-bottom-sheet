{
  "name": "@vaicar/react-native-bottom-sheet",
  "version": "1.0.1",
  "description": "Simple and fast bottom sheet for react-native",
  "main": "lib/commonjs/index.js",
  "module": "lib/module/index.js",
  "react-native": "src/index.ts",
  "types": "lib/typescript/index.d.ts",
  "files": [
    "lib/",
    "src/"
  ],
  "directories": {
    "example": "examples"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "prepare": "bob build",
    "prepublish": "tsc --noEmit",
    "release": "release-it"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/blongg/react-native-bottom-sheet.git"
  },
  "keywords": [
    "react-native",
    "bottom-sheet",
    "fast",
    "lightweight",
    "gesture",
    "reanimated",
    "gesture-handler"
  ],
  "peerDependencies": {
    "react": "*",
    "react-native": "*",
    "react-native-gesture-handler": "*",
    "react-native-reanimated": "*"
  },
  "author": "Matheus Diniz",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/blongg/react-native-bottom-sheet/issues"
  },
  "homepage": "https://github.com/blongg/react-native-bottom-sheet#readme",
  "devDependencies": {
    "@react-native-community/bob": "^0.10.1",
    "@types/react": "^16.9.34",
    "@types/react-native": "^0.62.5",
    "react-native-gesture-handler": "^1.6.1",
    "react-native-reanimated": "^1.8.0",
    "release-it": "^13.5.7",
    "typescript": "^3.8.3"
  },
  "@react-native-community/bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      "commonjs",
      "module",
      "typescript"
    ]
  }
}
